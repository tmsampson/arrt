{
	// See https://go.microsoft.com/fwlink/?LinkId=733558 
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "cargo",
			"subcommand": "check",
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"label": "Rust: cargo run [quality=lowest]",
			"group": "build",
			"type": "shell",
			"command": "cargo run -- --quality=lowest",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"label": "Rust: cargo run [quality=low]",
			"group": "build",
			"type": "shell",
			"command": "cargo run -- --quality=low",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"label": "Rust: cargo run [quality=medium]",
			"group": "build",
			"type": "shell",
			"command": "cargo run -- --quality=medium",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"label": "Rust: cargo run [quality=high]",
			"group": "build",
			"type": "shell",
			"command": "cargo run -- --quality=high",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		},
		{
			"label": "Rust: cargo run [quality=highest]",
			"group": "build",
			"type": "shell",
			"command": "cargo run -- --quality=highest",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$rustc"
			],
			"presentation": {
				"clear": true
			}
		}
	]
}